[
  {
    "kernelSource": {
      "name": "dm3q-KernelSU",
      "repo": "https://github.com/fei-ke/android_kernel_samsung_sm8550",
      "branch": "gki",
      "device": "dm3q",
      "defconfig": "kalama_gki_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "url": "http://sam.xiatianx.com/Kernel.tar.gz",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommands": {
        "LLVM": "1",
        "LLVM_IAS": "1",
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
